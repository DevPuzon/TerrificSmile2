                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                </ListView>
                            </ScrollViewer>
                            <TextBox Name="textbox_payment" materialDesign:HintAssist.Hint="Payment" materialDesign:HintAssist.IsFloating="True"  HorizontalAlignment="Left"  TextWrapping="Wrap" Width="180" PreviewTextInput="NumberValidationTextBox" LostFocus="Textbox_payment_LostFocus"/>
                            <TextBox Name="textbox_amount" materialDesign:HintAssist.Hint="Total Amount" materialDesign:HintAssist.IsFloating="True"  HorizontalAlignment="Left" IsEnabled="False"  TextWrapping="Wrap" Width="180" PreviewTextInput="NumberValidationTextBox"/>
                            <TextBox Name="textbox_change" materialDesign:HintAssist.Hint="Change" materialDesign:HintAssist.IsFloating="True"  HorizontalAlignment="Left" IsEnabled="False"  TextWrapping="Wrap" Width="180"/>
                            <Button Name="bttn_clear" Margin="5" Foreground="White" Background="#E91E63" Style="{DynamicResource MaterialDesignFlatButton}" Click="Bttn_clear_Click">
                                <StackPanel Orientation="Horizontal">
                                    <iconPacks:PackIconMaterial Kind="FormatClear"/>
                                    <TextBlock Text="Clear" Margin="10,0,0,0"/>
                                </StackPanel>
                            </Button>
                            <Button Name="bttn_done" Margin="5" Foreground="White" Background="#2196F3" Style="{DynamicResource MaterialDesignFlatButton}" Click="bttn_done_Click">
                                <StackPanel Orientation="Horizontal">
                                    <iconPacks:PackIconMaterial Kind="Check"/>
                                    <TextBlock Text="Done" Margin="10,0,0,0"/>
                                </StackPanel>
                            </Button>
                        </StackPanel>
                   </Grid>
               </Grid>
            </Grid>
        </Grid>
        <!--********** RECEIPT *********-->
        <Grid Name="grid_receipt" Grid.ColumnSpan="2" Background="Black" Visibility="Collapsed">
            <Grid Margin="350,30,350,30" Background="#E0E0E0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <StackPanel>
                    <Button Name="bttn_cancel" Margin="5" Foreground="White" Background="#E91E63" Style="{DynamicResource MaterialDesignFlatButton}" Click="bttn_cancel_Click">
                        <StackPanel Orientation="Horizontal">
                            <iconPacks:PackIconMaterial Kind="ArrowLeft"/>
                            <TextBlock Text="Cancel" Margin="10,0,0,0"/>
                        </StackPanel>
                    </Button>
                    <Button Name="bttn_print" Margin="5" Foreground="White" Background="#2196F3" Style="{DynamicResource MaterialDesignFlatButton}" Click="bttn_print_Click">
                        <StackPanel Orientation="Horizontal">
                            <iconPacks:PackIconMaterial Kind="Printer"/>
                            <TextBlock Text="Print" Margin="10,0,0,0"/>
                        </StackPanel>
                    </Button>
                </StackPanel>
                <Grid Grid.Column="1" >
                    <TextBlock Text="Official Receipt" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" />
                    <RichTextBox Name="richbox_receiptD" IsEnabled="False" Margin="10,30,10,10"/>
                </Grid>
            </Grid>
        </Grid>
        <!--********** //RECEIPT *********-->
    </Grid>
</UserControl>
